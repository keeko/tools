$serializer = {{ class }}::getSerializer();
$id = $serializer->getId($model->get{{ related }}());
if ($id !== null) {
	$relationship = new Relationship(new Resource($model->get{{ related }}(), $serializer));
	$relationship->setLinks([
		'related' => '%apiurl%' . $serializer->getType(null) . '/' . $id 
	]);
	return $this->addRelationshipSelfLink($relationship, $model, '{{ related_type }}');
}

return null;